<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assests\background-about.jpg" />
    <None Remove="Assests\background-cart.png" />
    <None Remove="Assests\background-contact.jpg" />
    <None Remove="Assests\background-home.jpg" />
    <None Remove="Assests\vanilla_cappuccino.jpg" />
    <None Remove="background-home.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assests\background-about.jpg" />
    <Resource Include="Assests\background-cart.png" />
    <Resource Include="Assests\background-contact.jpg" />
    <Resource Include="Assests\background-home.jpg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assests\vanilla_cappuccino.jpg" />
    <Resource Include="background-home.jpg" />
  </ItemGroup>

</Project>
